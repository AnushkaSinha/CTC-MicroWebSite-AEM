<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CTA - Title, Desc, Link"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <contactUs
                jcr:primaryType="nt:unstructured"
                jcr:title="Contact details"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <enqType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter title"
                                fieldLabel="Title"
                                name="./enqType"/>
                            <enqDetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter Name"
                                fieldLabel="Name"
                                name="./enqDetails"/>
                            <telLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter tel label"
                                fieldLabel="Tel Label"
                                name="./telLabel"/>
                            <telNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter tel number"
                                fieldLabel="Tel"
                                name="./telNumber"/>
                            <faxLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter fax label"
                                fieldLabel="Fax Label"
                                name="./faxLabel"/>
                            <faxNumber
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter fax"
                                fieldLabel="Fax"
                                name="./faxNumber"/>
                            <emailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Email Label"
                                name="./emailLabel"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Email"
                                name="./email"
                                regex="/^[A-za-z0-9]+[\\._]*[A-za-z0-9]*@[A-za-z.-]+[\\.]+[A-Za-z]{2,4}$/"/>
                            <emailSubject
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Email subject"
                                name="./emailSubject"/>
                        </items>
                    </column>
                </items>
            </contactUs>
            <spacingControl
                jcr:primaryType="nt:unstructured"
                jcr:title="Spacing Control"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <spacingstylestop
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Select top spacing for component"
                                fieldLabel="Top Spacing"
                                name="./spacingstylestop">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="Default"/>
                                    <spacingtop-0px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-0px"
                                        value="spacingtop-zeropx"/>
                                    <spacingtop-10px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-10px"
                                        value="spacingtop-10px"/>
                                    <spacingtop-20px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-20px"
                                        value="spacingtop-20px"/>
                                </items>
                            </spacingstylestop>
                            <spacingstylesbottom
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Select bottom spacing for component"
                                fieldLabel="Bottom Spacing"
                                name="./spacingstylesbottom">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="Default"/>
                                    <spacingbottom-0px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-0px"
                                        value="spacingbottom-zeropx"/>
                                    <spacingbottom-10px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-10px"
                                        value="spacingbottom-10px"/>
                                    <spacingbottom-20px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-20px"
                                        value="spacingbottom-20px"/>
                                </items>
                            </spacingstylesbottom>
                        </items>
                    </column>
                </items>
            </spacingControl>
        </items>
    </content>
</jcr:root>
