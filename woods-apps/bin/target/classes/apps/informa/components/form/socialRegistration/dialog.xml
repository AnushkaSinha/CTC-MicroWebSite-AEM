<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    activeTab="{Long}0"
    height="{Long}400"
    helpPath="en/cq/current/wcm/default_components.html#Forms Dropdown List"
    width="{Long}650"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <first
            jcr:primaryType="nt:unstructured"
            title="Social Links"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="cq:Widget"
                    defaultValue="Social Registration"
                    fieldLabel="Title"
                    name="./title"
                    xtype="textfield"/>
                <facebook
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Facebook"
                    inputValue="false"
                    name="./facebook"
                    type="checkbox"
                    xtype="selection"/>
                <twitter
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Twitter"
                    inputValue="false"
                    name="./twitter"
                    type="checkbox"
                    xtype="selection"/>
                <linkedIn
                    jcr:primaryType="cq:Widget"
                    fieldLabel="LinkedIn"
                    inputValue="false"
                    name="./linkedIn"
                    type="checkbox"
                    xtype="selection"/>
            </items>
        </first>
    </items>
</jcr:root>
