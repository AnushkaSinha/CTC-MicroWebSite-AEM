<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Login"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <signmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Sign In"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <titlemap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Titles"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Title"
                                fieldLabel="Title label"
                                name="./title"/>
                            <rememberMeLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Remember Me"
                                fieldLabel="Remember Me Label"
                                name="./rememberMeLabel"/>
                            <socialTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Social Title"
                                fieldLabel="Social Title Label"
                                name="./socialTitle"/>
                            <emailmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email"
                                fieldLabel="Email Label"
                                name="./email"/>
                            <reqEmail
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Email not entered"
                                fieldLabel="Required Error Message"
                                name="./reqEmail"/>
                            <passwordmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Password"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <password
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Password"
                                fieldLabel="Password Label"
                                name="./password"/>
                            <reqPassword
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Password not entered"
                                fieldLabel="Required Error Message"
                                name="./reqPassword"/>
                            <forgotpasswordmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Forgot Password"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <forgotPasswordLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Forgot Password"
                                fieldLabel="Forgot Password Label"
                                name="./forgotPasswordLabel"/>
                            <forgotpasswordUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide the path for the Forgot password page"
                                fieldLabel="Forgot Password Page Path"
                                name="./forgotpasswordUrl"
                                required="true"/>
                            <linksmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Links"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <buttonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Sign In Button"
                                fieldLabel="Sign in Label"
                                name="./buttonLabel"/>
                            <landingPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide the path for the Landing page"
                                fieldLabel="Landing page path "
                                name="./landingPage"
                                required="true"/>
                            <signupmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Sign Up"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <shortTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Short Title"
                                fieldLabel="Short Title Label"
                                name="./shortTitle"/>
                            <longTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Long Title"
                                fieldLabel="Long Title Label"
                                name="./longTitle"/>
                            <signupLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription=" Label for Sign Up Button "
                                fieldLabel="Sign Up Label"
                                name="./signupLabel"/>
                            <signupUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide the path for the Sign Up Page"
                                fieldLabel="Sign Up Page Path"
                                name="./signupUrl"
                                required="true"/>
                        </items>
                    </column>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Messages"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <invalidcredentialsmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Invalid Credentials"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <invalidCredentialsTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for Invalid Credentials"
                                fieldLabel="Invalid Credentials Title"
                                name="./invalidCredentialsTitle"/>
                            <invalidCredentialsMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message for Invalid Credentials"
                                fieldLabel="Invalid Credentials Message"
                                name="./invalidCredentialsMsg"/>
                            <unverifiedstatusmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Unverified Status"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <unverifiedStatusTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for Unverified Status"
                                fieldLabel="Unverified Status Title"
                                name="./unverifiedStatusTitle"/>
                            <emailActiveMain
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email Active Main"
                                fieldLabel="Email Active Main Label"
                                name="./emailActiveMain"/>
                            <emailActiveSub
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email Active Sub"
                                fieldLabel="Email Active Sub Label"
                                name="./emailActiveSub"/>
                        </items>
                    </columns>
                </items>
            </tab2>
        </items>
    </content>
</jcr:root>
