<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Personal Information">
                <items jcr:primaryType="cq:WidgetCollection">
                    <firstName
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="First Name Label"
                        name="./firstName"
                        xtype="textfield"/>
                    <lastName
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Last name Label"
                        name="./lastName"
                        xtype="textfield"/>
                    <dateofbirth
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Date of Birth Label"
                        name="./dateofbirth"
                        xtype="datefield"/>
                    <gender
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Gender label"
                        name="./gender"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select"/>
                            <male
                                jcr:primaryType="nt:unstructured"
                                text="Male"
                                value="{Long}1"/>
                            <female
                                jcr:primaryType="nt:unstructured"
                                text="Female"
                                value="{Long}2"/>
                        </options>
                    </gender>
                    <nationality
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Nationality Label"
                        name="./nationality"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select Nationality"
                                value="{Long}1"/>
                            <indian
                                jcr:primaryType="nt:unstructured"
                                text="Indian"
                                value="{Long}2"/>
                        </options>
                    </nationality>
                    <contact
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Contact Num Label"
                        name="./contact"
                        xtype="textfield"/>
                    <email
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Email ID Label"
                        name="./email"
                        xtype="textfield"/>
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Country Label"
                        name="./country"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select Country"
                                value="{Long}1"/>
                            <indian
                                jcr:primaryType="nt:unstructured"
                                text="Indian"
                                value="{Long}2"/>
                        </options>
                    </country>
                    <state
                        jcr:primaryType="cq:Widget"
                        fieldLabel="State Label"
                        name="./state"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select State"
                                value="{Long}1"/>
                            <karnataka
                                jcr:primaryType="nt:unstructured"
                                text="Karnataka"
                                value="{Long}2"/>
                        </options>
                    </state>
                    <address
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Address Label"
                        name="./address"
                        xtype="textarea"/>
                    <pinCode
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Pin Code label"
                        name="./pinCode"
                        xtype="textfield"/>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Company Information">
                <items jcr:primaryType="cq:WidgetCollection">
                    <companyName
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Company Name Label"
                        name="./companyName"
                        xtype="textfield"/>
                    <jobTitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Job Title Label"
                        name="./jobTitle"
                        xtype="textfield"/>
                    <industry
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Industry label"
                        name="./industry"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <industry
                                jcr:primaryType="nt:unstructured"
                                text="Industry"/>
                            <_x0031_
                                jcr:primaryType="nt:unstructured"
                                text="#01"
                                value="{Long}1"/>
                            <_x0032_
                                jcr:primaryType="nt:unstructured"
                                text="#02"
                                value="{Long}2"/>
                        </options>
                    </industry>
                    <department
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Department Label"
                        name="./department"
                        xtype="textfield"/>
                    <contact
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Contact Num Label"
                        name="./contact"
                        xtype="textfield"/>
                    <country
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Country Label"
                        name="./country"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select Country"
                                value="{Long}1"/>
                            <indian
                                jcr:primaryType="nt:unstructured"
                                text="Indian"
                                value="{Long}2"/>
                        </options>
                    </country>
                    <state
                        jcr:primaryType="cq:Widget"
                        fieldLabel="State Label"
                        name="./state"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select State"
                                value="{Long}1"/>
                            <karnataka
                                jcr:primaryType="nt:unstructured"
                                text="Karnataka"
                                value="{Long}2"/>
                        </options>
                    </state>
                    <address
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Address Label"
                        name="./address"
                        xtype="textarea"/>
                    <pinCode
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Pin Code label"
                        name="./pinCode"
                        xtype="textfield"/>
                </items>
            </tab2>
            <tab3
                jcr:primaryType="cq:Panel"
                title="Interest">
                <items jcr:primaryType="cq:WidgetCollection">
                    <titleOne
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Label"
                        name="./titleOne"
                        xtype="textfield"/>
                    <certification
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Certification Label"
                        name="./certification"
                        type="checkbox"
                        xtype="selection"/>
                    <exhibitions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Exhibitions Label"
                        name="./exhibitions"
                        xtype="checkbox"/>
                    <talks
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Talks Label"
                        name="./talks"
                        xtype="checkbox"/>
                    <sessions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sessions Label"
                        name="./sessions"
                        xtype="checkbox"/>
                    <workshop
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Workshop Label"
                        name="./workshop"
                        xtype="checkbox"/>
                    <titleTwo
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Label"
                        name="./titleTwo"
                        xtype="textfield"/>
                    <network
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Network Label"
                        name="./network"
                        type="checkbox"
                        xtype="selection"/>
                    <business
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Business Label"
                        name="./business"
                        xtype="checkbox"/>
                    <experience
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Experience Label"
                        name="./experience"
                        xtype="checkbox"/>
                    <investors
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Investors Label"
                        name="./investors"
                        xtype="checkbox"/>
                    <titleThree
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Label"
                        name="./titleThree"
                        xtype="textfield"/>
                    <cosmetics
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Cosmetics Label"
                        name="./cosmetics"
                        type="checkbox"
                        xtype="selection"/>
                    <oilGas
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Oil&amp;Gas Label"
                        name="./oilGas"
                        xtype="checkbox"/>
                    <machinery
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Machinery Label"
                        name="./machinery"
                        xtype="checkbox"/>
                    <telecommunication
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Telecommunication Label"
                        name="./telecommunication"
                        xtype="checkbox"/>
                </items>
            </tab3>
            <tab4
                jcr:primaryType="cq:Panel"
                title="Account Information">
                <items jcr:primaryType="cq:WidgetCollection">
                    <titleOne
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Label"
                        name="./titleOne"
                        xtype="textfield"/>
                    <question
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Question Label"
                        name="./question"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <select
                                jcr:primaryType="nt:unstructured"
                                text="Select the Question"/>
                            <ques1
                                jcr:primaryType="nt:unstructured"
                                text="Question01"
                                value="{Long}1"/>
                            <ques2
                                jcr:primaryType="nt:unstructured"
                                text="Question02"
                                value="{Long}2"/>
                        </options>
                    </question>
                    <answer
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Answer Label"
                        name="./answer"
                        xtype="textfield"/>
                    <titleTwo
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title Label"
                        name="./titleTwo"
                        xtype="textfield"/>
                    <password
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Password Label"
                        name="./password"
                        xtype="textfield"/>
                    <confirmPassword
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Confirm Password Label"
                        name="./confirmPassword"
                        xtype="textfield"/>
                </items>
            </tab4>
        </items>
    </items>
</jcr:root>
