<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Change Email"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Change Email"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Title"
                                fieldLabel="Title"
                                name="./title"/>
                            <email
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <emailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email"
                                fieldLabel="Email Label"
                                name="./email"/>
                            <reqEmail
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Email not entered"
                                fieldLabel="Required Error Message"
                                name="./reqEmail"/>
                            <confirmaEmailPage
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Confirm Email Page"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <confirmEmailPagePath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide the path for the Confirm Email page"
                                fieldLabel="Confirm Email Page Path"
                                name="./confirmEmailPage"
                                required="true"/>
                            <emailTemplate
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email Template"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <emailTemplatePath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide the path for Email Template page"
                                fieldLabel="Email Template Path"
                                name="./emailTemplate"
                                required="true"/>
                            <button
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Button Label"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <submit
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Submit Button"
                                fieldLabel="Submit Button Label"
                                name="./submit"/>
                            <cancel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Cancel Button"
                                fieldLabel="Cancel Button Label"
                                name="./cancel"/>
                            <update
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for UpdateButton"
                                fieldLabel="Update Button Label"
                                name="./update"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <messages
                jcr:primaryType="nt:unstructured"
                jcr:title="Messages"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <successMessage
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Success "
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <successTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for success message"
                                fieldLabel="Success Title"
                                name="./changeEmailSuccessTitle"/>
                            <successdescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message displayed when success"
                                fieldLabel="Success Message"
                                name="./changeEmailSuccessMsg"/>
                            <emailExistingMessage
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email Already Exist "
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <emailExistingTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title if email entered already exist"
                                fieldLabel="Email Already Exist Title"
                                name="./emailExistingTitle"/>
                            <emailExistingdescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message displayed when email entered already exist"
                                fieldLabel="Email Already Exist Message"
                                name="./emailExistingMessage"/>
                        </items>
                    </columns>
                </items>
            </messages>
        </items>
    </content>
</jcr:root>
