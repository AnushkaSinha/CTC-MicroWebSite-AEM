<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Use this template to import a Newsletter that will be delivered using Adobe Campaign."
    jcr:primaryType="cq:Template"
    jcr:title="Importer Page for Adobe Campaign"
    allowedPaths="/content/campaigns/geometrixx-outdoors/e-mails(/.*)?"
    ranking="{Long}1003">
    <jcr:content
        cq:designPath="/etc/designs/default"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="mcm/campaign/components/newsletter-landingpage"
        acMapping="mapRecipient"
        acTemplateId="mailAEMContent">
        <importer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/designimporter/components/importer"/>
    </jcr:content>
</jcr:root>
