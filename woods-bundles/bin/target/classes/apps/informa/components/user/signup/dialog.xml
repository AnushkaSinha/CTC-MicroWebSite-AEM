<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Sign In"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <emailID
            jcr:primaryType="cq:Widget"
            fieldLabel="Email ID Label"
            name="./emailID"
            xtype="textfield"/>
        <password
            jcr:primaryType="cq:Widget"
            fieldLabel="Password Label"
            name="./password"
            xtype="textfield"/>
        <submit
            jcr:primaryType="cq:Widget"
            fieldLabel="Submit Button Label"
            name="./submit"
            xtype="textfield"/>
        <firstName
            jcr:primaryType="cq:Widget"
            fieldLabel="First Name Label"
            name="./firstName"
            xtype="textfield"/>
        <lastName
            jcr:primaryType="cq:Widget"
            fieldLabel="Last name Label"
            name="./lastName"
            xtype="textfield"/>
        <businessLine
            jcr:primaryType="cq:Widget"
            fieldLabel="Business Line Label"
            name="./businessLine"
            xtype="textfield"/>
        <userName
            jcr:primaryType="cq:Widget"
            fieldLabel="User name Label"
            name="./userName"
            xtype="textfield"/>
        <editProfile
            jcr:primaryType="cq:Widget"
            fieldLabel="Enable Edit Profile"
            name="./editProfile"
            type="checkbox"
            xtype="selection"/>
        <loginPage
            jcr:primaryType="cq:Widget"
            fieldLabel="Login Page"
            name="./loginPage"
            xtype="pathfield"/>
        <address
            jcr:primaryType="cq:Widget"
            fieldLabel="Address Label"
            name="./address"
            xtype="textfield"/>
    </items>
</jcr:root>
