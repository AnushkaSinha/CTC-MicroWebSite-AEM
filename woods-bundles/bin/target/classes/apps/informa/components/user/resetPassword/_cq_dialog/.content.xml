<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Reset Password"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <titlemap
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Titles"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Title"
                        fieldLabel="Title label"
                        name="./title"/>
                    <passwordmap
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="New Password"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                    <newPassword
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for New Password"
                        fieldLabel="New Password Label"
                        name="./newPassword"/>
                    <reqPassword
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Provide Error message to be displayed if New Password not entered"
                        fieldLabel="Required Error Message"
                        name="./reqPassword"/>
                    <passwordStrength
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Provide Error message to be displayed if Password Strength is weak"
                        fieldLabel="Password Strength Error Message"
                        name="./passwordStrength"/>
                    <passwordRule
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                        fieldDescription="Label for Password Rules"
                        fieldLabel="Password Rules"
                        name="./passwordRule"/>
                    <conirmpasswordmap
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Confirm New Password"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                    <confirmPassword
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Confirm Password"
                        fieldLabel="Confirm Password Label"
                        name="./confirmPassword"/>
                    <reqConfirmPassword
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Provide Error message to be displayed if Confirm New Password not entered"
                        fieldLabel="Required Error Message"
                        name="./reqConfirmPassword"/>
                    <identicalPassword
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Provide Error message to be displayed if mismatch in Passwords"
                        fieldLabel="Required Error Message for Identical Password"
                        name="./identicalPassword"/>
                    <linksmap
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Links"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                    <buttonLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Submit Button"
                        fieldLabel="Submit Label"
                        name="./buttonLabel"/>
                    <redirectTo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldDescription="Provide Path for Redirect page"
                        fieldLabel="Redirect Page Path"
                        name="./redirectTo"
                        required="true"/>
                    <dynamicerrormap
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Dynamic Errors"
                        sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                    <successReset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Success Reset"
                        fieldLabel="Success Reset Label"
                        name="./successReset"/>
                    <passwordReset
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Password Reset"
                        fieldLabel="Password Reset Label"
                        name="./passwordReset"/>
                    <passwordCheck
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Label for Password Check"
                        fieldLabel="Password Check Label"
                        name="./passwordCheck"/>
                    <title.xml/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
