<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Personal Information"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#Text">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <editProfile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Check if Update Profile Mode"
                                name="./editProfile"
                                text="Update Profile"
                                value="true"/>
                            <firstnamemap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="First Name"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <firstName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for First Name"
                                fieldLabel="First Name Label"
                                name="./firstName"/>
                            <reqFirstName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if First Name not entered"
                                fieldLabel="Required Error Message"
                                name="./reqFirstName"/>
                            <checkFirstName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Invalid Characters  entered"
                                fieldLabel="Invalid Characters Error Message"
                                name="./checkFirstName"/>
                            <lastnamemap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Last Name"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <lastName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Last Name"
                                fieldLabel="Last Name Label"
                                name="./lastName"/>
                            <reqLastName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Last Name not entered"
                                fieldLabel="Required Error Message"
                                name="./reqLastName"/>
                            <checkLastName
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Invalid Characters  entered"
                                fieldLabel="Invalid Characters Error Message"
                                name="./checkLastName"/>
                            <dateofbirthmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Date of Birth"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <dateofbirth
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Date of Birth"
                                fieldLabel="Date of Birth Label"
                                name="./dateofbirth"/>
                            <contactmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Contact"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <contact
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Contact"
                                fieldLabel="Contact Label"
                                name="./contact"/>
                            <emailmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <email
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email"
                                fieldLabel="Email Label"
                                name="./emailID"/>
                            <userExistErrorMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if user already exist"
                                fieldLabel="User already Exist Error Message"
                                name="./userExistErrorMsg"/>
                            <reqEmail
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Email not entered"
                                fieldLabel="Required Error Message"
                                name="./reqEmail"/>
                            <statemap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="State"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <state
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for State"
                                fieldLabel="State Label"
                                name="./state"/>
                            <checkState
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Provide Error message to be displayed if Invalid Characters  entered"
                                fieldLabel="Invalid Characters Error Message"
                                name="./checkState"/>
                            <addressmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Address"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <address
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Address"
                                fieldLabel="Address Label"
                                name="./address"/>
                            <pincodemap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Pin Code"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <pincode
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Pin Code"
                                fieldLabel="Pin Code Label"
                                name="./pinCode"/>
                            <gendermap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Gender"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <genderLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Gender"
                                fieldLabel="Gender Label"
                                name="./genderLabel"/>
                            <genders
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/foundation/form/multifield"
                                aem-composit-multifield=""
                                class="full-width"
                                eaem-nested=""
                                fieldDescription="Add Gender"
                                fieldLabel="Gender List"
                                name="./genders">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    eaem-nested=""
                                    name="./genders">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resouceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <gender
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Gender"
                                                    name="./gender"/>
                                                <genderCode
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/hiddenfield"
                                                    fieldLabel="Gender Code"
                                                    name="./genderCode"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </genders>
                            <linksmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Links"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <next
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Next Button"
                                fieldLabel="Next Button Label"
                                name="./next"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Nationality"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <nationalitymap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Nationality"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <nationality
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Nationality"
                                fieldLabel="Nationality Label"
                                name="./nationality"/>
                            <nationalities
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/foundation/form/multifield"
                                aem-composit-multifield=""
                                class="full-width"
                                eaem-nested=""
                                fieldDescription="Add Nationalities"
                                fieldLabel="Nationality List"
                                name="./nationalities">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    eaem-nested=""
                                    name="./nationalities">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resouceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <nationality
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Nationality"
                                                    name="./nationality"/>
                                                <nationalityCode
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/hiddenfield"
                                                    fieldLabel="Nationality Code"
                                                    name="./nationalityCode"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </nationalities>
                        </items>
                    </columns>
                </items>
            </tab2>
            <tab3
                jcr:primaryType="nt:unstructured"
                jcr:title="Country"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <countrymap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Country"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <country
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Country"
                                fieldLabel="Country Label"
                                name="./country"/>
                            <countries
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/foundation/form/multifield"
                                aem-composit-multifield=""
                                class="full-width"
                                eaem-nested=""
                                fieldDescription="Add Countries"
                                fieldLabel="Country List"
                                name="./countries">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    eaem-nested=""
                                    name="./countries">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resouceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <country
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Country"
                                                    name="./country"/>
                                                <countryCode
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Country Code"
                                                    name="./countryCode"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </countries>
                        </items>
                    </columns>
                </items>
            </tab3>
            <messages
                jcr:primaryType="nt:unstructured"
                jcr:title="Messages"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <successMessage
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Success "
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <successTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for success message"
                                fieldLabel="Success Title"
                                name="./personalSuccessTitle"/>
                            <successdescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message displayed when success"
                                fieldLabel="Success Message"
                                name="./personalSuccessMsg"/>
                        </items>
                    </columns>
                </items>
            </messages>
        </items>
    </content>
</jcr:root>
