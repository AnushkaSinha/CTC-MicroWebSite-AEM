<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Confirm Email Change"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Title"
                                fieldLabel="Title"
                                name="./title"/>
                            <confirmaEmailPage
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Confirm Success Page"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <confirmEmailPagePath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Provide Path for Confirmation Success Page"
                                fieldLabel="Confirm Success Page Path"
                                name="./confirmSuccessPage"
                                required="true"/>
                            <email
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Email"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <emailLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Email"
                                fieldLabel="Label"
                                name="./email"/>
                            <reqEmail
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Required Error Message"
                                fieldLabel="Required Error Message"
                                name="./reqEmail"/>
                            <password
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Password"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <passwordLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Password"
                                fieldLabel="Password Label"
                                name="./password"/>
                            <reqPassword
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for Required Error Message"
                                fieldLabel="Required Error Message"
                                name="./reqFirstName"/>
                            <button
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Button Label"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <confirm
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Label for ConfirmButton"
                                fieldLabel="Confirm Button Label"
                                name="./confirm"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Messages"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <successmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Success"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <successTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for Success Message"
                                fieldLabel="Success Title"
                                name="./successTitle"/>
                            <successMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message displayed when Success"
                                fieldLabel="Success Message"
                                name="./successMsg"/>
                            <wrongcredentialsmap
                                cq:showOnCreate="{Boolean}false"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Wrong Credentials"
                                sling:resourceType="granite/ui/components/foundation/form/fieldset"/>
                            <wrongCredentialsTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for Wrong Credentials"
                                fieldLabel="Wrong Credentials Title"
                                name="./wrongCredentialsTitle"/>
                            <wrongCredentialsMsg
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Message for Wrong Credentials"
                                fieldLabel="Wrong Credentials Message"
                                name="./wrongCredentialsMsg"/>
                        </items>
                    </columns>
                </items>
            </tab2>
        </items>
    </content>
</jcr:root>
