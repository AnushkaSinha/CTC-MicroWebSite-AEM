<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Google Map"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#Text">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Event details"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <date
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="This is a free text field and is optional"
                                fieldLabel="Event date"
                                name="./eventDate"/>
                            <Label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="This is a free text field and is optional"
                                fieldLabel="Label"
                                name="./eventLabel"/>
                            <address
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Should be a physical address and not a google map link. This will get automatically displayed in the map"
                                fieldLabel="Address"
                                name="./address"/>
                            <latitude
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Should be a latitude of the physical address"
                                fieldLabel="Latitude"
                                name="./latitude"/>
                            <longitude
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Should be a longitude of the physical address"
                                fieldLabel="Longitude"
                                name="./longitude"/>
                            <externalUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./externalUrl"
                                text="External Url"
                                value="{Boolean}true"/>
                            <linkText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter link text. It is a free text field"
                                fieldLabel="Link text"
                                name="./linkText"/>
                            <linkPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Can either be a google maps link or link to a page within the website"
                                fieldLabel="Google maps link"
                                name="./linkPath"/>
                            <openUrlInNewWindow
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                name="./openUrlInNewWindow"
                                text="Open in new window"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Admission time"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Enter admission label. It is free text field"
                                fieldLabel="Admission label"
                                name="./admissionLabel"/>
                            <admission/>
                        </items>
                    </columns>
                </items>
            </tab2>
            <spacingControl
                jcr:primaryType="nt:unstructured"
                jcr:title="Spacing Control"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <spacingstylestop
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Select top spacing for component"
                                fieldLabel="Top Spacing"
                                name="./spacingstylestop">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="Default"/>
                                    <spacingtop-0px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-0px"
                                        value="spacingtop-zeropx"/>
                                    <spacingtop-10px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-10px"
                                        value="spacingtop-10px"/>
                                    <spacingtop-20px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Top-20px"
                                        value="spacingtop-20px"/>
                                </items>
                            </spacingstylestop>
                            <spacingstylesbottom
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Select bottom spacing for component"
                                fieldLabel="Bottom Spacing"
                                name="./spacingstylesbottom">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value="Default"/>
                                    <spacingbottom-0px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-0px"
                                        value="spacingbottom-zeropx"/>
                                    <spacingbottom-10px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-10px"
                                        value="spacingbottom-10px"/>
                                    <spacingbottom-20px
                                        jcr:primaryType="nt:unstructured"
                                        text="Spacing Bottom-20px"
                                        value="spacingbottom-20px"/>
                                </items>
                            </spacingstylesbottom>
                        </items>
                    </column>
                </items>
            </spacingControl>
            <stylesTab/>
        </items>
    </content>
</jcr:root>
