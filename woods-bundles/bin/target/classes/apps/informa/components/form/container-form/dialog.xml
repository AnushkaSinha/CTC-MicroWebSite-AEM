<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Form Type Selection"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <formType
            jcr:primaryType="cq:Widget"
            defaultValue="Default Form"
            fieldLabel="Form Type"
            name="./formType"
            title="Form Type"
            type="select"
            xtype="selection">
            <options jcr:primaryType="cq:WidgetCollection">
                <defaultRegistration
                    jcr:primaryType="cq:Widget"
                    text="Default Form"
                    value="Default Form"/>
                <visitorRegistration
                    jcr:primaryType="cq:Widget"
                    text="Visitor Registration"
                    value="Visitor Registration"/>
                <mediaRegistration
                    jcr:primaryType="cq:Widget"
                    text="Media Registration"
                    value="Media Registration"/>
                <delegateRegistration
                    jcr:primaryType="cq:Widget"
                    text="Delegate Registration"
                    value="Delegate Registration"/>
                <bookAStand
                    jcr:primaryType="cq:Widget"
                    text="Book A Stand"
                    value="Book A Stand"/>
                <visa
                    jcr:primaryType="cq:Widget"
                    text="Visa"
                    value="Visa"/>
            </options>
        </formType>
    </items>
</jcr:root>
