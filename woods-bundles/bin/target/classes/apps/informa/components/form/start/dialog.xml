<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="{Long}375"
    helpPath="en/cq/current/wcm/default_components.html#Form"
    stateful="{Boolean}false"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs
            jcr:primaryType="cq:TabPanel"
            layoutOnTabChange="{Boolean}true">
            <items jcr:primaryType="cq:WidgetCollection">
                <first
                    jcr:primaryType="nt:unstructured"
                    title="Form"
                    xtype="panel">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <emailcheck
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Enable Email Notification"
                            fieldLabel="Email"
                            name="./emailcheck"
                            type="checkbox"
                            xtype="selection"/>
                        <marketeremailcheck
                            jcr:primaryType="cq:Widget"
                            defaultValue="{Boolean}true"
                            fieldDescription="Enable Marketer Email Notification"
                            fieldLabel="Marketer Email"
                            name="./marketerEmailCheck"
                            type="checkbox"
                            xtype="selection"/>
                        <from
                            jcr:primaryType="cq:Widget"
                            fieldLabel="From"
                            name="./from"
                            xtype="textfield"/>
                        <mailTo
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Specify Marketeer Email Address."
                            fieldLabel="Mail To"
                            name="./mailTo"
                            xtype="multifield"/>
                        <cc
                            jcr:primaryType="cq:Widget"
                            fieldLabel="CC"
                            name="./cc"
                            xtype="multifield"/>
                        <bcc
                            jcr:primaryType="cq:Widget"
                            fieldLabel="BCC"
                            name="./bcc"
                            xtype="multifield"/>
                        <subject
                            jcr:primaryType="cq:Widget"
                            fieldLabel="Subject"
                            name="./subject"
                            xtype="textfield"/>
                        <template
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Specify End User Email Template Path."
                            fieldLabel="End User Email Template"
                            name="./template"
                            xtype="pathfield"/>
                        <thankyouPage
                            jcr:primaryType="nt:unstructured"
                            fieldDescription="Specify Thank You Page Path."
                            fieldLabel="Thank You Page"
                            name="./thankyouPage"
                            rootPath="/content/informa"
                            xtype="pathfield"/>
                        <paymentRedirection
                            jcr:primaryType="nt:unstructured"
                            fieldDescription="Specify Payment Redirection Path."
                            fieldLabel="Payment Redirection"
                            name="./paymentRedirection"
                            rootPath="/content/informa"
                            xtype="pathfield"/>
                        <marketerTemplate
                            jcr:primaryType="cq:Widget"
                            fieldDescription="Specify Marketer Email Template Path."
                            fieldLabel="Marketer Email Template"
                            name="./marketerTemplate"
                            xtype="pathfield"/>
                    </items>
                </first>
            </items>
        </tabs>
    </items>
</jcr:root>
